{
  "name": "n8n-nodes-maisa-worker",
  "version": "2.0.0",
  "description": "N8N node for Maisa Worker API integration with polling and file handling",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "maisa",
    "worker",
    "ai"
  ],
  "license": "MIT",
  "homepage": "https://github.com/juanpedrolozano-cyber/MaisaWorkern8n",
  "author": {
    "name": "Juan Pedro Lozano",
    "email": "juanpedro.lozano@maisa.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanpedrolozano-cyber/MaisaWorkern8n.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MaisaWorkerApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/MaisaWorker/MaisaWorker.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "form-data": "^4.0.0"
  }
}
